<?xml version='1.0' encoding='utf-8'?>
<widget id="com.fm.lapatriada" version="1.1.3" versionCode="60" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FM La Patriada</name>
    <description>
        FM Radio La Patriada
    </description>
    <author email="ricardodanielvil@gmail.com" href="https://github.com/rickvil">
        ricardodanielvil@gmail.com
    </author>
    <content src="index.html" />
    <icon src="www/img/icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="deployment-target" value="7.0" />
    </platform>
    <allow-navigation href="*" />
    <allow-navigation href="https://*/*" />
    <preference name="Orientation" value="portrait" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="FadeSplashScreenDuration" value="0" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="SplashScreen" value="none" />
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="webviewbounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <allow-navigation href="http://rnadmin.xicnet.com/*" />
    <access launch-external="yes" origin="whatsapp:*" />
    <engine name="android" spec="~5.1.1" />
    <engine name="ios" spec="~4.1.1" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
